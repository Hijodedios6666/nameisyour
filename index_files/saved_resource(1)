(function getBasketItem(){
    const interval = setInterval(function() {  
        getItem();             
    }, 400);
        const getItem = () => {
        //  const img = document.querySelector('.cart-item__image-container');
        const title = document.querySelector('.reservation-cart-container_roomRate a');
        const price = document.querySelector('.price-summary_price');
        const date = document.querySelector('.reservation-cart-container_dates ');
        if (date && title && price){
            const obj = {
                // img: img.src,
                date: date.innerText,
                title: title.textContent.trim(),
                price: price.textContent.trim()
            };
            const itemEncoded = btoa(encodeURIComponent(JSON.stringify(obj)));
            $smcT5.CookieManager.create('smc_dyn_item_code', itemEncoded, 0);
            $smcT5.LS.set('smc_dyn_item_ls', itemEncoded)
        }   
    };
})();
    
    
  (() => {
    const interval = setInterval(function() {  
        const cookie = $smcT5.CookieManager.read('smc_dyn_item_code');
	    if (!cookie) setDynBasketFromLs();          
    }, 400);
	const setDynBasketFromLs = () => {
		$smcT5.LS.get('smc_dyn_item_ls', (data) => {
			if(data) {
				$smcT5.CookieManager.create('smc_dyn_item_code', data);
			}
		});

	}

})();

